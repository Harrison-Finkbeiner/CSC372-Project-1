<p style="color: green"><%= notice %></p>

<h1> Sign in </h1>

<%= form_for :user do |f| %>
  <p>
    <%= f.label :Username %><br>
    <%= f.text_field :username %>
  </p>

  <p>
    <%= f.label :Password %><br>
    <%= f.password_field :password %>
  </p>


  <%= f.submit "Sign in" %>
  <%= link_to "Create Account", new_user_path %>

<% end %>